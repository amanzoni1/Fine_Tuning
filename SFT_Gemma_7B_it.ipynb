        "# Define your terms and prepare a list of (term, sentiment, count)\n",
        "financial_terms = [\n",
        "    'revenue', 'profit', 'loss', 'earnings', 'growth', 'decline',\n",
        "    'stock', 'share', 'dividend', 'quarter', 'market', 'sales',\n",
